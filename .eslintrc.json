{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
            "vue/multi-word-component-names": ["error", {
              "ignores": [
                  "gallery",
                  "index",
                  "Tutorial"
              ]
            }],
            "space-before-function-paren": [
                "error",
                "never"
            ],
            "comma-dangle": [
                "error",
                "never"
            ]
          }
          
    }

